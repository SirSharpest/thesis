@article{medina-pucheNovelPathwayLinking2019,
  title = {A Novel Pathway Linking Plasma Membrane and Chloroplasts Is Co-Opted by Pathogens to Suppress Salicylic Acid-Dependent Defences},
  author = {{Medina-Puche}, Laura and Tan, Huang and Dogra, Vivek and Wu, Mengshi and {Rosas-Diaz}, Tabata and Wang, Liping and Ding, Xue and Zhang, Dan and Fu, Xing and Kim, Chanhong and {Lozano-Duran}, Rosa},
  year = {2019},
