}

@article{Gonzalez2011,
  title = {Wheat Floret Survival as Related to Pre-Anthesis Spike Growth},
  author = {Gonz{\'a}lez, Fernanda G. and Miralles, Daniel J. and Slafer, Gustavo A.},
  year = {2011},
  volume = {62},
