@article{miyaCERK1LysMReceptor2007,
  title = {{{CERK1}}, a {{LysM}} Receptor Kinase, Is Essential for Chitin Elicitor Signaling in {{Arabidopsis}}},
  author = {Miya, Ayako and Albert, Premkumar and Shinya, Tomonori and Desaki, Yoshitake and Ichimura, Kazuya and Shirasu, Ken and Narusaka, Yoshihiro and Kawakami, Naoto and Kaku, Hanae and Shibuya, Naoto},
  year = {2007},
