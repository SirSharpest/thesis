@article{edelEvolutionCalciumBasedSignalling2017,
  title = {The {{Evolution}} of {{Calcium}}-{{Based Signalling}} in {{Plants}}},
  author = {Edel, Kai H. and Marchadier, Elodie and Brownlee, Colin and Kudla, J{\"o}rg and Hetherington, Alistair M.},
  year = {2017},
