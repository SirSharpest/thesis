@article{stahlTranscriptomicAnalysisCave2015,
  title = {A {{Transcriptomic Analysis}} of {{Cave}}, {{Surface}}, and {{Hybrid Isopod Crustaceans}} of the {{Species Asellus}} Aquaticus},
