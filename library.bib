  isbn = {1664-462X},
  journal = {Frontiers in Plant Science},
  keywords = {anthesis,heat stress,meiosis,ppd-d1,Ppd-D1,rht,Rht,wheat},
  note = {00009},
  number = {January},
  pmid = {28179910}
}

