  isbn = {1072201399},
  journal = {Genetic Resources and Crop Evolution},
  keywords = {Chloroplast SSRs,cpSSRs,Genetic diversity,Polyploid wheat,Wheat,Wheat evolution},
  number = {6},
  pmid = {19386614}
}

@incollection{levyChapterCytology132009,
  title = {Chapter 4.4.2 - {{Cytology}} of the (1-3)-{$\beta$}-{{Glucan}} ({{Callose}}) in {{Plasmodesmata}} and {{Sieve Plate Pores}}},
  booktitle = {Chemistry, {{Biochemistry}}, and {{Biology}} of 1-3 {{Beta Glucans}} and {{Related Polysaccharides}}},
