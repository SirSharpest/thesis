@article{leePlasmodesmataLocalizedProteinMediates2011,
  title = {A {{Plasmodesmata}}-{{Localized Protein Mediates Crosstalk}} between {{Cell}}-to-{{Cell Communication}} and {{Innate Immunity}} in {{Arabidopsis}}},
  author = {Lee, Jung-Youn and Wang, Xu and Cui, Weier and Sager, Ross and Modla, Shannon and Czymmek, Kirk and Zybaliov, Boris and van Wijk, Klaas and Zhang, Chong and Lu, Hua and Lakshmanan, Venkatachalam},
