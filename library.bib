@article{brunkardPlantCellcellTransport2019,
  title = {Plant Cell-Cell Transport via Plasmodesmata Is Regulated by Light and the Circadian Clock},
  author = {Brunkard, Jacob O. and Zambryski, Patricia C.},
  year = {2019},
  month = jan,
  issn = {0032-0889, 1532-2548},
  doi = {10.1104/pp.19.00460},
  abstract = {Plasmodesmata (PD) are essential for plant development, but little is known about their regulation. Several studies have linked PD transport to chloroplast-centered signaling networks, but the physiological significance of this connection remains unclear. Here, we show that PD transport is strongly regulated by light and the circadian clock. Light promotes PD transport during the day, but light is not sufficient to increase rates of PD transport at night, suggesting a circadian gating mechanism. Silencing expression of the core circadian clock gene, LHY/CCA1, allows light to strongly promote PD transport during subjective night, confirming that the canonical plant circadian clock controls the PD transport light response. We conclude that PD transport is dynamically regulated during the day/night cycle. Due to the many roles of PD in plant biology, this discovery has strong implications for plant development, physiology, and pathogenesis.},
  copyright = {\{copyright, serif\} 2019 American Society of Plant Biologists. All rights reserved..},
