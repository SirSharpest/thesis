@article{hickmanArchitectureDynamicsJasmonic2017,
  title = {Architecture and {{Dynamics}} of the {{Jasmonic Acid Gene Regulatory Network}}},
  author = {Hickman, Richard and {van Verk}, Marcel C. and Van Dijken, Anja J.H. and Pereira Mendes, Marciel and {Vroegop-Vos}, Irene A. and Caarls, Lotte and Steenbergen, Merel and Van Der Nagel, Ivo and Wesselink, Gert Jan and Jironkin, Aleksey and Talbot, Adam and Rhodes, Johanna and {de Vries}, Michel and Schuurink, Robert C. and Denby, Katherine and Pieterse, Corn{\'e} M.J. and Van Wees, Saskia C.M.},
